{
  "name": "@qualweb/qw-page",
  "version": "0.2.2",
  "description": "Utilities module for qualweb",
  "main": "dist/qw-page.bundle.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "tsc": "tsc",
    "test": "mocha --require esm",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prebuild": "rimraf prebuild && npm run format && tsc --build",
    "build": "npm run prebuild && rimraf dist && webpack --mode production && rimraf prebuild",
    "docs": "typedoc --out docs src/index.ts",
    "prepare": "npm run build"
  },
  "homepage": "https://github.com/qualweb/qw-page#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qualweb/qw-page.git"
  },
  "bugs": {
    "url": "https://github.com/qualweb/qw-page/issues"
  },
  "keywords": [
    "qualweb",
    "accessibility",
    "qualweb-page"
  ],
  "author": "Jo√£o Vicente",
  "license": "ISC",
  "devDependencies": {
    "@qualweb/dom": "^0.2.0",
    "@qualweb/types": "^0.5.3",
    "@tsconfig/recommended": "^1.0.1",
    "@types/puppeteer": "^5.4.2",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "chai": "^4.3.4",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-sonarjs": "^0.6.0",
    "esm": "^3.2.25",
    "mocha": "^8.3.2",
    "prettier": "^2.1.2",
    "puppeteer": "^8.0.0",
    "rimraf": "^3.0.2",
    "typedoc": "^0.20.34",
    "typescript": "^4.2.3",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.3.1"
  },
  "dependencies": {
    "@qualweb/qw-element": "^0.2.2"
  }
}
